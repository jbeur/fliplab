{
  "name": "fliplab-scraper-service",
  "version": "1.0.0",
  "description": "Standalone web scraper service for Facebook Marketplace and Poshmark data",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "scraper",
    "facebook-marketplace",
    "poshmark",
    "fliplab",
    "web-scraping",
    "api"
